{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec \"ts-node -r tsconfig-paths/register\" src/server.ts",
    "start": "node -r tsconfig-paths/register dist/src/server.js",
    "build": "rm -rf dist && tsc",
    "postinstall": "npm run build && npm run db:migrate",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "seed:admin": "npx ts-node src/sequilizedir/seeders/admin.seeder.ts",
    "seed:hotel": "npx ts-node src/sequilizedir/seeders/hotelSetting.seeder.ts"
  },
